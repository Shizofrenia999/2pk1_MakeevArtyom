using System;

public class Program
{
	public static int[] Progres(int n)
	{
		int[] result = new int[n];
		for (int i = 0; i < n; i++)
		{
			int x = 10 * (int)Math.Pow(2, i);
			result[i] = x;
		}

		return result;
	}

	public static void Main()
	{
		int[] progression = Progres(5);
		foreach (int num in progression)
		{
			Console.WriteLine(num);
		}
	}
}
